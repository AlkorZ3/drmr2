    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 2;
    lv2:symbol "right";
    lv2:name "Right";
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 3;
    lv2:symbol "gain_one";
    lv2:name "Sample One Gain";
    lv2:minimum 0.0;
    lv2:maximum 1.0;
    lv2:default 1.0;
  ],

